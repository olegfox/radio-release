{% extends '::base.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" type="text/css"
          href="http://fonts.googleapis.com/css?family=Actor:400|Open+Sans:300,300italic,400,400italic,600,600italic,700,700italic,800,800italic|Oswald:300|Source+Sans+Pro:200&subset=latin,cyrillic">
    <link rel="stylesheet" href="{{ asset('sitemain/js/jquery-colorbox/example3/colorbox.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('sitemain/css/style.css') }}">
{% endblock %}
{% block body %}
    {{ parent() }}
    <div class="content">
        {{ render(url('Site_main_audio')) }}
        {% block content %}
        {% endblock %}
    </div>
    {# Forms #}
    {{ render(url('frontend_email')) }}
    {# end Forms #}
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('sitemain/js/jquery-1.10.2.min.js') }}"></script>
    <script src="{{ asset('sitemain/js/jquery-colorbox/jquery.colorbox-min.js') }}"></script>
    <script src="{{ asset('sitemain/js/jquery-form/jquery.form.js') }}"></script>
    <script src="{{ asset('sitemain/js/jquery-validation/dist/jquery.validate.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('sitemain/js/script.js') }}"></script>
    <script type="text/javascript" src="{{ asset('sitemain/js/angular.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('sitemain/js/audio5.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('sitemain/js/init.audio.js') }}"></script>
{% endblock %}